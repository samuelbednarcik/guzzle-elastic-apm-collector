{
    "name": "samuelbednarcik/guzzle-elastic-apm-collector",
    "description": "Elastic APM span collector for Guzzle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Bednarcik",
            "email": "bednarciks@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "samuelbednarcik/elastic-apm-agent": "^0.2.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "SamuelBednarcik\\ElasticAPMAgent\\Collectors\\Guzzle\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5"
    },
    "scripts": {
      "phpstan": "phpstan analyse src --level 7"
    },
    "abandoned": "philkra/elastic-apm-php-agent"
}
